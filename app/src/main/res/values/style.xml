<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="primary_landingpage" >
        <item name="android:textSize">20dp</item>
        <item name="android:layout_marginRight">82dp</item>
        <item name="android:layout_marginTop">43px</item>
        <item name="android:layout_marginLeft">82dp</item>
        <item name="android:fontFamily">@font/oswald</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="desc_landingpage">
        <item name="android:textSize">11sp</item>
        <item name="android:letterSpacing">0.1</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:layout_marginLeft">82dp</item>
        <item name="android:layout_marginTop">43px</item>
        <item name="android:layout_marginRight">82dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:fontFamily">@font/questrial_regular</item>
        <item name="android:importantForAccessibility">no</item>
    </style>

    <style name="landingpageimg">
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
    </style>

    <style name="footerlandingimg">
        <item name="android:width">374px</item>
        <item name="android:height">90px</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:gravity">bottom </item>
        <item name="android:translationY">70dp</item>
    </style>

    <style name="logoimg">
        <item name="maxHeight">100dp</item>
        <item name="maxWidth">88dp</item>
        <item name="android:paddingLeft">37dp</item>
        <item name="android:paddingEnd">37dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <style name="wordmark">
        <item name="android:layout_height">25dp</item>
        <item name="android:width">174dp</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:textSize">32sp</item>
        <item name="android:layout_marginEnd">14.5dp</item>
        <item name="android:layout_marginLeft">14.5dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="fontFamily">@font/josefinsans_bold</item>
        <item name="android:textColor">@color/red</item>
    </style>

    <style name="desc_wordmark">
        <item name="android:layout_height">25dp</item>
        <item name="android:width">174dp</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginEnd">14.5dp</item>
        <item name="android:layout_marginLeft">14.5dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="fontFamily">@font/josefinsans_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="button">
        <item name="android:colorButtonNormal">@color/blue</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:shape">rectangle</item>
        <item name="android:radius">6dp</item>
        <item name="android:width">240dp</item>
        <item name="android:height">50dp</item>
    </style>

    <style name="smallbutton">
        <item name="android:colorButtonNormal">@color/blue</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:shape">rectangle</item>
        <item name="android:radius">6dp</item>
        <item name="android:width">100dp</item>
        <item name="android:height">46dp</item>
    </style>

    <style name="layout">
        <item name="android:layout_width">316dp</item>
        <item name="android:layout_height">650dp</item>
        <item name="android:width">219dp</item>
        <item name="android:height">50dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_marginStart">40dp</item>
        <item name="android:layout_marginTop">37dp</item>
        <item name="android:layout_marginBottom">44dp</item>
        <item name="android:layout_marginEnd">40dp</item>
    </style>

    <style name="tagline">
        <item name="android:textColor">#000000</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingEnd">98dp</item>
        <item name="android:lineSpacingExtra">3sp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="fontFamily">@font/josefinsans_semibold</item>
    </style>

    <style name="columninput">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@drawable/columninput</item>
        <item name="android:fontFamily">@font/josefinsans_light</item>
        <item name="android:textSize">13dp</item>
    </style>

    <style name="smallcolin">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@drawable/smallcolin</item>

    </style>

    <style name="asklogin">
        <item name="android:layout_marginTop">11dp</item>
        <item name="android:textSize">11dp</item>
        <item name="android:width">203dp</item>
        <item name="android:height">46dp</item>
        <item name="fontFamily">@font/josefinsans_light</item>
    </style>

    <style name="namapengguna">
        <item name="fontFamily">@font/josefinsans_bold</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#FCFCFC</item>
    </style>
    



</resources>